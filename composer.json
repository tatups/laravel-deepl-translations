{
    "name": "blacklabelbytes/laravel-deepl-translations",
    "version": "1.1.1",
    "description": "Generates translation files using DeepL API",
    "type": "library",
    "license": "MIT",
    "require": {
        "babymarkt/deepl-php-lib": "^3.0"
    },
    "authors": [
        {
            "name": "Tatu Puskala",
            "email": "tatu@blb.fi"
        }
    ],
    "autoload": {
        "psr-4": {
            "BlackLabelBytes\\Translations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackLabelBytes\\Translations\\DeepLTranslationServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.21",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}